{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel_app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "SESSION_SECRET": "vercel-production-secret-key-2024"
  },
  "functions": {
    "vercel_app.py": {
      "runtime": "python3.9"
    }
  }
}
